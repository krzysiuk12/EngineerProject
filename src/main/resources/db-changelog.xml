<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <include file="liquibase/db-sequences.xml" />
    <include file="liquibase/db-changelog-1.0.xml" />
    <include file="liquibase/db-changelog-1.1.xml" />

    <changeSet id="DATALOAD-001" author="kkicinger">
        <loadData tableName="accountsecurityprofiles" schemaName="${schemaName}" file="data/accountsecurityprofiles.csv" separator="," />
        <loadData tableName="passwordsecurityprofiles" schemaName="${schemaName}" file="data/passwordsecurityprofiles.csv" separator="," />
        <loadData tableName="securityprofiles" schemaName="${schemaName}" file="data/securityprofiles.csv" separator="," />
        <loadData tableName="individuals" schemaName="${schemaName}" file="data/individuals.csv" separator="," />
        <loadData tableName="useraccounts" schemaName="${schemaName}" file="data/users.csv" separator="," />
        <loadData tableName="locations" schemaName="${schemaName}" file="data/locations.csv" separator="," />
    </changeSet>
    
</databaseChangeLog>